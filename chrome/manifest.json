{
        "name": "NEM NanoWallet",
        "description": "NEM NanoWallet for browser extension.",
        "version": "0.0.2",
        "manifest_version": 2,
        "icons": {
                "16": "logo16.png",
                "48": "logo48.png",
                "96": "logo96.png",
                "128": "logo128.png"
        },
        "permissions": ["tabs"],
				"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
        "background": {
                "presistant": false,
                "scripts": ["chrome.js"]
        },
        "browser_action": {
                "default_title": "NanoWallet"
        }
}
